<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mkx_app">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MKXAPP">
        <activity
            android:name=".acyivitys.PublishActivity"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".acyivitys.ProfileActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".acyivitys.DashboardActivity"
            android:exported="true"></activity>
        <activity
            android:name=".acyivitys.SigninActivity"
            android:exported="true" />
        <activity
            android:name=".acyivitys.MainActivity"
            android:exported="true"
            android:theme="@style/Theme.MKXAPP.login_style" />
    </application>

</manifest>